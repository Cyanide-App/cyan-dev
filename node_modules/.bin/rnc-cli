#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/project/workspace/node_modules/@react-native-community/cli/build/node_modules:/project/workspace/node_modules/@react-native-community/cli/node_modules:/project/workspace/node_modules/@react-native-community/node_modules:/project/workspace/node_modules:/project/node_modules:/node_modules:/project/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/project/workspace/node_modules/@react-native-community/cli/build/node_modules:/project/workspace/node_modules/@react-native-community/cli/node_modules:/project/workspace/node_modules/@react-native-community/node_modules:/project/workspace/node_modules:/project/node_modules:/node_modules:/project/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@react-native-community/cli/build/bin.js" "$@"
else
  exec node  "$basedir/../@react-native-community/cli/build/bin.js" "$@"
fi
