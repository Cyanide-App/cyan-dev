{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Content-Security-Policy",
          "value": "script-src 'self' 'unsafe-eval';"
        }
      ]
    }
  ],
  "functions": {
    "app/**/*.wasm": {
      "headers": {
        "Content-Type": "application/wasm"
      }
    }
  }
}