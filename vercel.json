{
    "version": 2,
    "devCommand": "node index.js",
    "builds": [{
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
            "prebuild": "mkdir -p node_modules && find node_modules -type d -exec mkdir -p /var/task/{} \\;"
        }
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "index.js"
    }]
  }